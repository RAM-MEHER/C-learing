                                                                                 //count of elements in a string.
#include<stdio.h>
#include<string.h>
int main(){
    char string[100];
    scanf("%[^\n]s" , string);
   int i , count = 0 ;
   for(i = 0 ; i < strlen(string) ; i++){
       if(string[i] == ' ') continue;
       else count++;
   }
   printf("%i" , count);
    return 0;
}
